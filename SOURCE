local FinishRun = game:GetService('ReplicatedStorage').Functions.FinishRun local namecall; namecall = hookmetamethod(game, '__namecall', function(caller, ...) local args = {...} local method = getnamecallmethod() if not checkcaller() and caller == FinishRun and method == 'InvokeServer' then args[2][1] = 1000  args[2][2] = 1000  args[2][3] = 100  args[2][8]['Breaks'] = 0xFFFFFFFFFFFFFFFF end return namecall(caller, unpack(args)) end)
